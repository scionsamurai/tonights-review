{
	"name": "sveltekit-blog2",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"generate-thumbnails": "node generate-thumbnails.js",
        "generate-post-groups": "node generate-post-groups.js",
        "prebuild": "npm run generate-thumbnails && npm run generate-post-groups",
		"predev": "npm run generate-thumbnails && npm run generate-post-groups",
		"dev": "vite dev --host --port 3000",
		"start": "node build",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-cloudflare": "^4.7.0",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "^8.56.7",
		"eslint": "^9.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.36.0",
		"globals": "^15.0.0",
		"mdsvex": "^0.11.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.77.8",
		"sharp": "^0.33.5",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.0.0-alpha.20",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"flexsearch": "^0.7.43",
		"gray-matter": "^4.0.3",
		"mdast-util-to-string": "^4.0.0",
		"rehype-stringify": "^10.0.0",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.0",
		"remark-stringify": "^11.0.0",
		"shiki": "^1.10.3",
		"unified": "^11.0.5",
		"unist-builder": "^4.0.0",
		"unist-util-map": "^4.0.0",
		"unist-util-visit": "^5.0.0"
	}
}
